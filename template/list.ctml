<article class="list" lquery='(add-class (when editable "editable")) (data :id id :order order)'>
  <ol class="items" iterate="items">
    <li data-id="1" lquery="(text text) (data :id id)">ITEM</li>
  </ol>
  <div class="images" iterate="items">
    <img data-id="1" src="#"
         lquery="(attr :src image :title text :alt text) (data :id id)"/>
  </div>
</article>
