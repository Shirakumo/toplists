<article class="list" lquery='(data :id _id :order (clip (** :order) :_id)) (add-class (when editable "editable"))'>
  <ol class="items" iterate="items">
    <li data-id="1" lquery="(data :id id) (text text)">
      <input type="hidden" name="item[]" lquery="(val id)" />
    </li>
  </ol>
  <div class="images" iterate="items">
    <img data-id="1" src="#"
         lquery="(attr :src image :title text :alt text) (data :id id)"/>
  </div>
</article>
